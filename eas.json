{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "image": "sdk-51"
      },
      "env": {
        "EXPO_USE_UPDATES": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "sdk-51"
      },
      "env": {
        "EXPO_USE_UPDATES": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "image": "sdk-51"
      },
      "env": {
        "EXPO_USE_UPDATES": "1"
      }
    }
  }
}
